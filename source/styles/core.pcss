.toggle-open.open .visible-open-inline{display: inline;}
.clearfix::after{content: ''; display: table; clear: both;}
.clear{clear: both;}

.text-left{text-align: left;}
.text-right{text-align: right;}
.text-center{text-align: center;}
.text-justify{text-align: justify;}

.sticky-top,
.sticky-bottom{position: -webkit-sticky; position: sticky;}
.sticky-stop{position: absolute;}



/* --- vue transitions --- */
.content-fade-enter-active, .content-fade-leave-active{transition: opacity $transition-base;}
.content-fade-enter, .content-fade-leave-to{opacity: 0;}

.content-fade-delayed-enter-active, .content-fade-delayed-leave-active{transition: opacity $transition-base ease $transition-base;}
.content-fade-delayed-enter, .content-fade-delayed-leave-to{opacity: 0;}

.content-scale-enter-active, .content-scale-leave-active{transition: $transition-base; transform: translate3d(0,0,0) scale(1);}
.content-scale-enter, .content-scale-leave-to{opacity: 0; transform: translate3d(0,0,0) scale(0);}

.dropdown-enter-active, .dropdown-leave-active{transition: $transition-dropdown; -webkit-transform-origin: center top; transform-origin: center top;}
.dropdown-enter, .dropdown-leave-to{transition: $transition-dropdown; opacity: 0; -webkit-transform: scaleY(0); transform: translate3d(0,0,0) scaleY(0);
	-webkit-transform-origin: center top; transform-origin: center top;}

.swipe-away-enter-active, .swipe-away-leave-active{transition: $transition-base;}
.swipe-away-enter, .swipe-away-leave-to{opacity: 0; -webkit-transform: scaleX(100%); transform: translate3d(100%,0,0);}


