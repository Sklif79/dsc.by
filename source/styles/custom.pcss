/* --- status --- */
.status {
  padding: 0 12px;
  font-size: $fz-xs;
  line-height: 24px;
  font-weight: 400;
  position: relative;
  color: $color-white;
  background: #007ebe;
  border: none;
  border-radius: 3px;
}


.status--new,
.status--novinka {
  background: #007ebe;
  color: $color-white;
}

.status--new::after,
.status--novinka::after {
  border-left-color: #007ebe;
}

.status--hit {
  background: #f7a300;
  color: $color-white;
}



.status--saving,
.status--sale {
  background: #ed1920;
  color: $color-white;
}

.status--saving::after,
.status--sale::after {
  border-left-color: #ed1920;
}

.status--lite {
  padding: 0 10px;
  line-height: 18px;
}

.status--lite::after {
  display: none;
}


/* --- status group --- */
.status-group .status {
  float: left;
  clear: both;
}


/* --- catalog list --- */
.catalog-grid__item {
  margin-bottom: 0;
}


/* --- catalog item --- */
.catalog-item__title {
  font-size: $fz-h2;
  line-height: $lh-h2;
}


/* --- social link --- */
.social-link--facebook {
  color: #3b5998;
}

.no-touch .social-link--facebook:hover {
  color: #3b5998;
  opacity: .7;
}

.social-link--twitter {
  color: #1DA1F2;
}

.no-touch .social-link--twitter:hover {
  color: #1DA1F2;
  opacity: .7;
}

.social-link--instagram {
  color: #000;
}

.no-touch .social-link--instagram:hover {
  color: #000;
  opacity: .7;
}

.social-link--vkontakte {
  color: #507299;
}

.no-touch .social-link--vkontakte:hover {
  color: #507299;
  opacity: .7;
}


/* --- btn --- */
/*
.btn--soc-facebook{color: #3b5998;}
.btn--soc-twitter{color: #1DA1F2;}
.btn--soc-vk{color: #507299;}
.btn--soc-instagram{color: #000;}
*/


/* --- sign sm --- */
.dropdown .sign-sm__sign-up {
  background: $color-gray-light-2;
}


/* ---- rich text input --- */
.rich-text-input__label.rich-text-input__label.rich-text-input__label {
  color: $color-black;
}


/* --- dropdown --- */
.dropdown-arrow-owner::before {
  left: 20px;
  margin-left: 0;
}


/* --- page heaer info block --- */
.page-header-info-block__cell-icon {
  padding-right: 10px;
}

.page-header-info-block__cell-arrow {
  display: table-cell;
  vertical-align: middle;
  padding-left: 12px;
  width: 1px;
}

.page-header-info-block__cell-arrow .svg-icon {
  width: 14px;
  height: 14px;
  transition: -webkit-transform $transition-base;
  transition: transform $transition-base;
  transform: translate3d(0, 0, 0);
}

.page-header-top-item__value,
.page-header-top-item__local-text {
  border-bottom: 1px dotted;
  color: $color-primary-alt;
}

.page-header-top-item__ext-text {
  border-bottom: 1px solid;
}

.page-header-info-block.open .page-header-info-block__cell-arrow .svg-icon {
  -webkit-transform: rotate(180deg);
  transform: translate3d(0, 0, 0) rotate(180deg);
}

.page-header-main__cell--search .product__status:not(:first-child), .page-header-main__cell--search .product__code{/*display: none;*/}
 .basket-action__cell-remove {top: -40px!important;}
.top-search-results__list .product__discount, .dropdown--basket .product__discount{font-size: 10px;width: 25px;height: 25px;top: -10px;right: -10px; z-index: 20;}
.top-search-results__list .product__status, .basket-sm__list .product__status{padding: 0 3px; font-size: 10px; line-height: 18px;}

/* --- product --- */
/*.product--list-item.product--basket-mode .product__action .basket-action{padding: 10px 15px; margin: -10px -15px;}*/
.product__no-image .svg-icon {
  width: 80px;
  height: 80px;
}


/* --- product-price-ext --- */
.product-price-ext__no-image {
  background: #fff;
  width: 100%;
  height: 100%;
  font-size: 0;
  text-align: center;
  box-shadow: 0 0 0 1px $color-gray-light inset;
}

.product-price-ext__no-image::after {
  content: '';
  display: inline-block;
  vertical-align: middle;
  height: 100%;
}

.product-price-ext__no-image .svg-icon {
  width: 60px;
  height: 60px;
  color: $color-gray-light;
}


/* --- page prefooter --- */
/*.page-prefooter{overflow: hidden; -webkit-transform: translate3d(0,0,0); transform: translate3d(0,0,0);}*/
.page-prefooter .container {
  position: relative;
  z-index: 2;
}

/*.page-prefooter::before{content: ''; position: absolute; top: 50%; left: 0; right: 0; height: 1000px; height: 100vmax; background: url(/local/images/prefooter-pattern.svg) repeat;
    -webkit-transform: translate(0,-50%) rotate(-15deg); transform: translate(0,-50%) rotate(-15deg); pointer-events: none;}*/
.page-prefooter .text-input {
  border-color: $color-white;
  background-color: $color-white;
}

.no-touch .page-prefooter .text-input:hover {
  background-color: $color-white;
}

.no-touch .page-prefooter .text-input:focus,
.page-prefooter .text-input:focus {
  background-color: $color-white;
}


/* --- number input --- */
.number-input__input .text-input {
  height: 40px;
  padding: 4px 20px;
}

.number-input__btn {
  height: 40px;
}

.number-input--lg .number-input__btn {
  height: 50px;
  width: 30px;
}

.number-input--lg .text-input {
  height: 50px;
  padding: 12px 30px;
}


/* --- menu btn --- */
.page-header-main__cell--menu-btn .menu-btn {
  width: 70px;
  height: 70px;
  margin: 0;
  background: $color-primary-alt;
}

.page-header-main__cell--menu-btn .menu-btn__part {
  background: $color-white;
}

.no-touch .page-header-main__cell--menu-btn .menu-btn:hover {
  border-color: $color-primary;
}

.no-touch .page-header-main__cell--menu-btn .menu-btn:hover .menu-btn__part {
  background: $color-primary;
}


/* --- floor banner --- */
.floor-banner {
  background: no-repeat center / auto 100%;
  background-size: cover;
  min-height: 470px;
  padding: 80px 0;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -ms-align-items: center;
  -webkit-align-items: center;
  align-items: center;
}

.floor-banner .container {
  width: 100%;
}

.floor-banner__content {
  max-width: 600px;
}

.floor-banner__title,
.floor-banner__text {
  margin: 20px 0;
}

.floor-banner__action {
  margin: 40px 0 20px;
}


/* --- custom-i10s --- */
.floor--custom-i10s {
  margin: 60px 0;
}

.custom-i10s-grid {
  margin: 0 -12px -25px -13px;
  width: calc(100% + 25px);
}

.custom-i10s-grid__item {
  padding: 0 12px 0 13px;
  margin-bottom: 25px;
}


/* --- custom-i10s --- */
.custom-i10s {
  text-align: center;
  font-size: $fz-base;
  line-height: $lh-base;
  border: 1px dashed $color-gray-light;
  border-radius: $border-radius-base;
  -ms-flex-positive: 1;
  -webkit-flex: 1 0 auto;
  flex: 1 0 auto;
  padding: 30px 30px 40px;
}

.custom-i10s__visual {
  height: 120px;
  max-width: 120px;
  margin: 15px auto;
  font-size: 0;
}

.custom-i10s__visual::after {
  content: '';
  display: inline-block;
  vertical-align: middle;
  height: 100%;
}

.custom-i10s__visual img {
  display: inline-block;
  vertical-align: middle;
  max-width: 100%;
  max-height: 100%;
}

.custom-i10s__title {
  margin: 8px auto;
  max-width: 310px;
}

.custom-i10s__title,
.custom-i10s__text {
  margin: 8px auto;
  max-width: 250px;
}


/* --- aside info block --- */
.aside-info-block {
  border-radius: $border-radius-base;
  padding: 25px;
  background-color: $color-gray-light-2;
  font-size: $fz-small;
  line-height: $lh-small;
  overflow: hidden;
}

.aside-info-block__grid {
  margin: -26px -26px -25px -25px;
  width: calc(100% + 51px);
}

.aside-info-block__group {
  padding: 25px;
  border-top: 1px solid $color-gray-light;
  border-right: 1px solid $color-gray-light;
  flex: 1 1 auto;
  font-size: 14px;
}


/* --- ig title --- */
.ig-title {
  display: table;
  width: 100%;
  table-layout: fixed;
  margin: 15px 0;
}

.ig-title__text {
  display: table-cell;
  vertical-align: middle;
}

.ig-title__icon {
  display: table-cell;
  vertical-align: top;
  width: 34px;
  padding-right: 10px;
  line-height: 0;
}

.ig-title__icon .svg-icon {
  width: 24px;
  height: 24px;
  vertical-align: middle;
  color: $color-gray;
  margin-top: 1px;
}


/* --- inline separator --- */
.inline-separator {
  display: inline-block;
  vertical-align: middle;
  line-height: 0;
  height: 1em;
  border-left: 1px solid $color-gray;
  margin: 0 19px;
}


/* --- category grid --- */
.cat-grid {
  border: 1px solid $color-gray-light;
  border-radius: $border-radius-base;
  overflow: hidden;
}

.cat-grid__grid {
  margin: 0 -1px -1px 0;
  width: calc(100% + 1px);
}

.cat-grid__item {
  border-right: 1px solid $color-gray-light;
  border-bottom: 1px solid $color-gray-light;
  padding: 30px 30px 35px;
  font-size: $fz-base;
}

.cat-grid__item-image {
  display: block;
  height: 140px;
  font-size: 0;
  border: none;
  margin: 0 0 18px;
  text-align: center;
}

.cat-grid__item-image::after {
  content: '';
  display: inline-block;
  vertical-align: middle;
  height: 100%;
}

.cat-grid__item-image img {
  display: inline-block;
  vertical-align: middle;
  max-width: 100%;
  max-height: 100%;
}
.cat-grid__image-box {
  width: 100%;
  height: 200px;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.cat-grid__item-link {
  display: block;
  border-bottom: none!important;
  color: #000;
}
.no-touch .cat-grid__item-link:hover .cat-grid__item-title{
  color: $color-red;
}
.no-touch .cat-grid__item-link:hover .cat-grid__image{
  transform: scale(1.05,1.05);
}
.cat-grid__image {
  width: auto;
  max-width: 100%;
  height: auto;
  max-height: 100%;
  transition: .1s;
}

.cat-grid__item-title {
  font-size: 18px;
  line-height: 22px;
  text-align: center;
  margin-bottom: 10px;
  min-height: calc(2 * $lh-h2);
  max-height: calc(2 * $lh-h2);
  overflow: hidden;
}

.cat-grid__item-title-link {
  color: $color-black;
}

.no-touch .cat-grid__item-image:hover {
  border: none;
}

.cat-grid__item-links-row {
  margin-bottom: 6px;
}

.cat-grid__item-links-row:last-child {
  margin-bottom: 0;
}

.cat-grid__item-links-more:not(.open) {
  display: none;
}

.cat-grid__item-link {
  color: $color-black;
  border: none;
}

.cat-grid__item-links-trigger {
  font-size: $fz-xs;
  line-height: $lh-xs;
  margin-top: 10px;
}

.cat-grid__item-links-trigger .svg-icon {
  transition: -webkit-transform $transition-base;
  transition: transform $transition-base;
}

.cat-grid__item-links-trigger .link.open .svg-icon {
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
}

.no-touch .cat-grid__item-link:hover {
  color: $link-color-hover;
  border: none;
}


/* --- page footer --- */
.page-footer .contact-info--tel {
  margin: 0;
}

.page-footer .contact-info--link,
.page-footer .contact-info-time,
.page-footer .contact-info-btns__item {
  font-size: $fz-xs;
  line-height: $lh-xs;
  color: $color-gray-dark;
}

.page-header-main__row .contact-info-time {
  min-width: 240px;
}

.page-footer .contact-info-btns__item {
  margin-right: 20px;
}

.page-footer .contact-info-btns {
  margin-top: 10px;
  margin-right: -20px;
}

.page-footer-text {
  font-size: $fz-xs;
  line-height: $lh-xs;
  color: $color-gray-dark;
}

.no-touch .page-footer .pay-icons {
  -webkit-filter: grayscale(100%);
  filter: grayscale(100%);
  opacity: .5;
}

.no-touch .page-footer .pay-icons:hover {
  -webkit-filter: grayscale(0%);
  filter: grayscale(0%);
  opacity: 1;
}


/* --- social buttons --- */
.social-buttons {
  margin: 0 -10px -10px 0;
  font-size: 0;
}

.social-buttons .btn {
  margin: 0 10px 15px 0;
  vertical-align: top;
  border: 0 !important;
}


/* --- info art block ---  */
.info-art-block {
  margin-top: -1px;
  border-top: 1px solid $body-bg;
}

.info-art-block__grid {
  width: calc(100% + 50px);
  margin: 0 -25px -50px;
}

.info-art-block__col {
  padding: 0 25px;
  margin-bottom: 50px;
  font-size: $fz-base;
}

.info-art-block__col::before {
  content: '';
  display: block;
  border-top: 1px solid $color-gray-light;
  margin-bottom: 30px;
}


/* --- info art item ---  */
.info-art-item {
  display: -webkit-flex;
  display: flex;
  -webkit-flex: 1 1 auto;
  flex: 1 1 auto;
  -webkit-flex-direction: column;
  flex-direction: column;
  word-break: break-word;
}

.info-art-item__head {
  display: table;
  width: 100%;
  table-layout: fixed;
  margin-bottom: 25px;
}

.info-art-item__head-icon {
  display: table-cell;
  vertical-align: top;
  width: 85px;
  padding-right: 15px;
}

.info-art-item__head-icon img {
  display: inline-block;
  vertical-align: middle;
  max-width: 100%;
  max-height: 70px;
}

.info-art-item__head-title {
  display: table-cell;
  vertical-align: middle;
}

.info-art-item__text {
  margin: 20px 0;
  flex-grow: 1000;
}

.info-art-item__person {
  margin: 28px 0;
}

.info-art-item__text + .info-art-item__person {
  margin-top: 8px;
}

.info-art-item__head + .info-art-item__text {
  margin-top: 0;
}


/* --- person --- */
.person {
  display: table;
  width: 100%;
  table-layout: fixed;
}

.person__cell-image {
  display: table-cell;
  vertical-align: top;
  width: 175px;
  padding-right: 35px;
}

.person__cell-main {
  display: table-cell;
  vertical-align: top;
}

.person__image {
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  max-width: 140px;
  border-radius: 50%;
  background: $color-gray-light no-repeat center / 100% auto;
  background-size: cover;
}

.person__image::before {
  content: '';
  display: block;
  padding-top: 100%;
  font-size: 0;
  line-height: 0;
}

.person__name {
  font-weight: 600;
}

.person__info {
  color: $color-gray-dark;
}

.person__contact,
.person__name,
.person__info {
  margin: 15px 0;
}

.person__action {
  margin: 25px 0;
}

.person__name + .person__info {
  margin-top: -15px;
}


/* --- file link --- */
.file-link {
  word-break: break-word;
}

.file-link,
.no-touch .file-link:hover {
  color: $text-color;
  transition: none;
  border: none;
}

.file-link__title-text {
  border-bottom: $link-border;
  transition: $transition-link;
  color: $link-color;
}

.file-link__meta,
.file-link__descr {
  font-size: $fz-small;
  line-height: $lh-small;
  margin: 8px 0;
}

.file-link__meta {
  color: $color-gray-dark;
}

.no-touch .file-link:hover .file-link__title-text {
  color: $link-color-hover;
  border-bottom: $link-border-hover;
}

.dop-forms-contacts {
  border: 1px solid $color-gray-light;
  margin-top: 0;
}
.checkbox--compare {position: relative; z-index: 15;}
.contacts-info strong {font-size: 16px; font-weight: 500;}


@media (min-width: $screen-md-min) {
  .layout__main > .info-art-block:first-child,
  .layout__main > *:first-child > .info-art-block:first-child {
    margin-top: -21px;
  }
}


@media (max-width: $screen-md-max) {
  /* --- page header main --- */
  .page-header-main__cell--contact {
    display: none;
  }
  .page-header-main__cell--contact + .page-header-main__cell {
    padding-left: 0;
  }


  /* --- floor banner --- */
  .floor-banner {
    min-height: calc(200px + 21vw);
  }


  /* --- custom-i10s --- */
  .custom-i10s {
    padding: 20px 20px 30px;
  }


  /* --- person --- */
  .person--narrow {
    text-align: center;
  }
  .person--narrow,
  .person--narrow .person__cell-image,
  .person--narrow .person__cell-main {
    display: block;
    width: auto;
    padding: 0;
  }
  .person--narrow .person__cell-image {
    margin-bottom: 25px;
  }

}

.basket-product-box--photo {flex-basis: 18%; max-width: 18%;    margin: 0;
  padding-right: 15px;}
.basket-product-box--name {flex-basis: 32%; padding-right: 15px;}
.basket-product-box--count {flex-basis: 20%;}
.basket-product-box--price {flex-basis: 25%;}
.basket-product-box--remove {flex-basis: 5%; z-index: 80; cursor: pointer;}



@media (max-width: $screen-sm-max) {
  .breadcrumbs-block {
    display: block;
  }
  .breadcrumbs-block .page-header-menu__item {display: none!important;}
  .breadcrumbs-block  .breadcrumbs {flex-wrap: wrap; height: auto; width: 100vw; margin: 15px -10px; padding: 10px;}
  .breadcrumbs-block  .breadcrumbs:after {display: none;}
  .breadcrumbs__link {font-size: 10px!important;}
  span.breadcrumbs__link {color: $color-primary-alt!important;font-size: 12px!important;}
    /* --- catalog top params --- */
  .catalog-top-params {
    position: -webkit-sticky;
    position: sticky;
    top: 0px;
    z-index: 80;
  }


  /* --- top search --- */
  .top-search__btn {
    padding-left: 10px;
    padding-right: 10px;
  }


  /* --- floor-banner --- */
  .floor-banner {
    padding: 60px 0;
  }


  /* --- custom-i10s --- */
  .floor--custom-i10s {
    margin: 60px 0;
  }
  .custom-i10s-grid {
    margin: 0 -10px -20px;
    width: calc(100% + 20px);
  }
  .custom-i10s-grid__item {
    padding: 0 10px;
    margin-bottom: 20px;
  }


  /* --- cat grid --- */
  .cat-grid__item {
    padding: 25px 20px 30px;
  }
}


@media (max-width: $screen-xs-max) {
  /* --- catalog item --- */
  .catalog-item__title {
    font-size: $fz-h2-xs;
    line-height: $lh-h2-xs;
  }


  /* --- page header main --- */
  /*.page-header-main__cell--menu-btn .menu-btn{width: 40px; height: 40px;}*/


  /* --- floor-banner --- */
  .floor-banner {
    padding: 30px 0 50px;
  }


  /* --- custom-i10s --- */
  .floor--custom-i10s {
    margin: 30px 0;
  }
  .custom-i10s__visual {
    max-width: 100px;
    height: 100px;
  }
}

@media (max-width: $screen-2xs-max) {
  /* --- cat grid --- */
  .cat-grid__item {
    padding: 25px 10px 30px;
  }
}


@media (max-width: $screen-3xs-max) {
  .cat-grid__item-title {
    min-height: 0;
    margin-bottom: 20px;
  }
}

.content-loaded.load-hide-content {
  visibility: visible;
}

.htmlblockedit {
  display: inline-block;
  width: auto;
  background-color: #ff0;
  font-size: 11px;
  line-height: 15px;
  border: 1px solid #f00;
  color: #00f;
  padding: 5px;
  cursor: pointer;
  position: relative;
  z-index: 100;
  font-weight: bold;
}

.part-left {
  position: relative;
  min-height: 20px;
}

.part-right {

}

.floor--slider {
  width: 100%;
  margin-bottom: 0;
}

.section-info-main {
  margin: 50px 0;
}

.section-title {
  font-size: 22px;
  font-weight: bold;
  line-height: 36px;
}

.section-title:after {
  content: '\203A';
  color: $color-red;
  font-size: 30px;
  padding-left: 7px;
  font-weight: 100;
  position: relative;
  bottom: -1px;
}

.info-main-blocks {
  padding-left: 40px;
}

.info-main-block {
  width: 50%;
  display: inline-flex;
  align-items: center;
  margin-bottom: 25px;
}

.info-main-block img {
  max-width: 70px;
  max-height: 70px;
  margin-right: 30px;
}

.section-news-main {
  background: rgba(115, 133, 149, 0.1);
  padding: 40px 0;
  margin: 20px 0;
}

/*.section-news-main>.container{
    max-width: 1222px;
}*/

.section-news-main_title {

}

.section-news-main_items {
  display: flex;
}

.section-news-main_item {
  display: inline-block;
  width: 25%;
  padding: 0 9px;
  max-height: 335px;
  overflow: hidden;
}

.section-news-main_item img {
  display: block;
  width: 100%;
}

.section-news-main_item span {
  display: block;
  font-size: 13px;
  padding: 11px 0 2px;
